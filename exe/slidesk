var SD_svg_default = `<svg xmlns="http://www.w3.org/2000/svg" version="1.0" viewBox="0 0 500 500">
    <rect width="100%" height="100%" fill="black" />
    <path fill="white" stroke="white"
        d="M0 250v250h500V0H0v250zm214.6-106.9c16.8 2.3 31.4 8.9 36.7 16.5 3.7 5.3 4.2 12.9 1.1 17-2.3 3.2-4.4 4.2-4.4 2.1 0-.7-1.4-4.1-3.1-7.5-8-16-28.4-26.2-52.7-26.2-27.1 0-44.5 11.5-48.2 31.8-1.8 10.1.1 16.5 6.6 22.2 6.8 5.9 15.2 8.5 45.4 13.9 30.4 5.5 43.5 10.5 53.1 20 7.9 7.9 10.3 14.4 10.3 27.6 0 14-2.3 19.6-11.8 29-8.2 8.2-13.7 11.3-26.1 15.1-8.4 2.6-10.2 2.8-27 2.8-15.4 0-19.3-.3-27-2.2-23.5-5.9-37.5-18.2-37.5-33 0-3 .7-6.8 1.5-8.6 2.6-5.5 9.7-8.2 14.6-5.6 1.8 1 1.8 1.2-.7 3.3-2 1.8-2.8 3.5-3.1 7.4-.7 6.6 2.2 13 8.9 19.5 25.9 25.3 79.8 20.9 93.8-7.7 2.1-4.3 2.5-6.4 2.5-15.5 0-12-1.3-15.8-7.7-22.2-7.6-7.6-17.4-11-48.3-16.4-32.1-5.6-41.9-9-50.2-17.6-6.7-6.8-8.8-12.5-8.8-23.8 0-10.7 1.8-16 8-23.6 6.8-8.2 22-15.9 36.5-18.3 7.8-1.3 28.5-1.3 37.6 0zm118.9 4.8c25.6 8.4 43.2 26.6 50.7 52.7 2 6.8 2.3 10.1 2.3 24.4 0 18.8-1.1 24.3-7.5 37.6-9.4 19.4-26.9 34.2-47.5 40-8.2 2.3-52.5 3.4-52.5 1.3 0-1.1 3.5-1.4 17.3-1.5 19.6-.1 26.7-1.4 38.3-7 22.1-10.8 36.4-34.5 38.9-64.4 1.3-15.5-2.4-33-9.8-47.5-5.1-9.9-17.2-22.5-26.2-27.2-12.1-6.3-18.1-7.5-39.2-8.1-16.1-.4-19.3-.7-19.3-1.9 0-1.3 3.3-1.4 23.8-1.1 21.3.4 24.4.7 30.7 2.7z" />
</svg>`;

// src/core/Server.js
var { log } = console;

class Server {
  static create(html, options, path2) {
    globalThis.html = html;
    globalThis.path = path2;
    globalThis.server = Bun.serve({
      port: options.port,
      fetch(req) {
        const url = new URL(req.url);
        if (url.pathname === "/")
          return new Response(globalThis.html.index.html, {
            headers: {
              "Content-Type": "text/html"
            }
          });
        if (url.pathname.match(/^\/--(\w+)--\/$/g))
          return new Response(globalThis.html[url.pathname.replaceAll("-", "").replaceAll("/", "")].html, {
            headers: {
              "Content-Type": "text/html"
            }
          });
        if (url.pathname === "/favicon.svg")
          return new Response(SD_svg_default, {
            headers: { "Content-Type": "image/svg+xml" }
          });
        if (url.pathname === "/notes")
          return new Response(layout_html_default2.replace("/* #SOCKETS# */", `window.slidesk.io = new WebSocket("ws://localhost:${options.port}/ws");`).replace("/* #STYLES# */", theme_css_default).replace("/* #SV_STYLES# */", styles_css_default).replace("/* #SV_SCRIPT# */", script_js_default + (options.gamepadSv ? gamepad_js_default2 : "")), {
            headers: {
              "Content-Type": "text/html"
            }
          });
        if (url.pathname === "/ws")
          return globalThis.server.upgrade(req) ? undefined : new Response("WebSocket upgrade error", { status: 400 });
        const fileurl = req.url.replace(`http://localhost:${options.port}`, "");
        const file = Bun.file(fileurl.match(/https?:\/\/(www\.)?[-a-zA-Z0-9@:%._+~#=]{1,256}\.[a-zA-Z0-9()]{1,6}\b([-a-zA-Z0-9()@:%_+.~#?&//=]*)/g) ? fileurl : `${globalThis.path}${fileurl}`);
        if (file.size !== 0)
          return new Response(file, {
            headers: {
              "Content-Type": file.type
            }
          });
        return new Response(`${req.url} not found`, { status: 404 });
      },
      websocket: {
        open(ws) {
          ws.subscribe("slidesk");
        },
        message(ws, message) {
          ws.publish("slidesk", message);
        },
        close(ws) {
          ws.unsubscribe("slidesk");
        }
      }
    });
    if (options.notes)
      log(`Your speaker view is available on: ${source_default.green.bold(`http://localhost:${options.port}/notes`)}`);
    log(`Your presentation is available on: ${source_default.blue.bold(`http://localhost:${options.port}`)}`);
    log();
  }
  static setHTML(html) {
    globalThis.html = html;
    globalThis.server.publish("slidesk", JSON.stringify({ action: "reload" }));
  }
  static send(action) {
    globalThis.server.publish("slidesk", JSON.stringify({ action }));
  }
}

// src/command/_common.js
var displayHeader = () => `
\u2591\u2588\u2588\u2588\u2588\u2588\u2588\u2557\u2588\u2588\u2557\u2591\u2591\u2591\u2591\u2591\u2588\u2588\u2557\u2588\u2588\u2588\u2588\u2588\u2588\u2557\u2591\u2588\u2588\u2588\u2588\u2588\u2588\u2588\u2557\u2591\u2588\u2588\u2588\u2588\u2588\u2588\u2557\u2588\u2588\u2557\u2591\u2591\u2588\u2588\u2557
\u2588\u2588\u2554\u2550\u2550\u2550\u2550\u255D\u2588\u2588\u2551\u2591\u2591\u2591\u2591\u2591\u255A\u2550\u255D\u2588\u2588\u2554\u2550\u2550\u2588\u2588\u2557\u2588\u2588\u2554\u2550\u2550\u2550\u2550\u255D\u2588\u2588\u2554\u2550\u2550\u2550\u2550\u255D\u2588\u2588\u2551\u2591\u2588\u2588\u2554\u255D
\u255A\u2588\u2588\u2588\u2588\u2588\u2557\u2591\u2588\u2588\u2551\u2591\u2591\u2591\u2591\u2591\u2588\u2588\u2551\u2588\u2588\u2551\u2591\u2591\u2588\u2588\u2551\u2588\u2588\u2588\u2588\u2588\u2557\u2591\u2591\u255A\u2588\u2588\u2588\u2588\u2588\u2557\u2591\u2588\u2588\u2588\u2588\u2588\u2550\u255D\u2591
\u2591\u255A\u2550\u2550\u2550\u2588\u2588\u2557\u2588\u2588\u2551\u2591\u2591\u2591\u2591\u2591\u2588\u2588\u2551\u2588\u2588\u2551\u2591\u2591\u2588\u2588\u2551\u2588\u2588\u2554\u2550\u2550\u255D\u2591\u2591\u2591\u255A\u2550\u2550\u2550\u2588\u2588\u2557\u2588\u2588\u2554\u2550\u2588\u2588\u2557\u2591
\u2588\u2588\u2588\u2588\u2588\u2588\u2554\u255D\u2588\u2588\u2588\u2588\u2588\u2588\u2588\u2557\u2588\u2588\u2551\u2588\u2588\u2588\u2588\u2588\u2588\u2554\u255D\u2588\u2588\u2588\u2588\u2588\u2588\u2588\u2557\u2588\u2588\u2588\u2588\u2588\u2588\u2554\u255D\u2588\u2588\u2551\u2591\u255A\u2588\u2588\u2557
\u255A\u2550\u2550\u2550\u2550\u2550\u255D\u2591\u255A\u2550\u2550\u2550\u2550\u2550\u2550\u255D\u255A\u2550\u255D\u255A\u2550\u2550\u2550\u2550\u2550\u255D\u2591\u255A\u2550\u2550\u2550\u2550\u2550\u2550\u255D\u255A\u2550\u2550\u2550\u2550\u2550\u255D\u2591\u255A\u2550\u255D\u2591\u2591\u255A\u2550\u255D
                                            v${package_default.version}
`;
var _common_default = displayHeader;

// src/command/present.js
var joliTitle = source_default.hex("#16cedd");
var { log: log2, error } = console;
var flow = (talk, options = {}, init = false) => {
  Interpreter.convert(`./${talk}/main.sdf`, options).then(async (html) => {
    if (options.save) {
      Object.entries(html).forEach(([key, value]) => {
        Bun.write(`./${talk}/${key}.html`, value.html);
      });
    } else if (init) {
      Server.create(html, options, `${process2.cwd()}/${talk}`);
    } else {
      Server.setHTML(html);
    }
  }).catch((err) => error(err));
};
var rl = readline.createInterface({
  input: process2.stdin,
  output: process2.stdout
});
var getAction = () => {
  rl.question("", (answer) => {
    const i = answer.trim().toLowerCase();
    readline.moveCursor(process2.stdout, 0, -1);
    readline.clearScreenDown(process2.stdout);
    if (i === "q")
      process2.exit();
    else if (i === "p")
      Server.send("previous");
    else
      Server.send("next");
    getAction();
  });
};
var present = (talk, options) => {
  log2(joliTitle(_common_default()), source_default.underline("\n\nTake the control of your presentation direct from here."), "\n", `\nPress ${source_default.italic("Enter")} to go to the next slide.`, `\nPress ${source_default.italic("P + Enter")} to go to the previous slide.`, `\nPress ${source_default.italic("Q")} to quit the program.`, "\n");
  flow(talk, options, true);
  if (!options.save) {
    watch(talk, { recursive: true }, (eventType, filename) => {
      if (!filename.startsWith(".git")) {
        log2(`\u267B\uFE0F  ${source_default.underline(filename)} has "${source_default.italic(eventType)}" action`);
        flow(talk, options);
      }
    });
  }
  getAction();
};
var present_default = present;

// src/command/create.js
var import_slugify = __toESM(require_slugify(), 1);
import {existsSync, mkdirSync} from "fs";
var prompts = require_prompts3();
var joliTitle2 = source_default.hex("#16cedd");
var { log: log3 } = console;
var create = async (talk) => {
  log3(joliTitle2(_common_default()), `Creation of your talk: ${talk}`);
  let dirName = import_slugify.default(talk);
  if (dirName === "create") {
    dirName = `${dirName}_`;
  }
  const response = await prompts([
    {
      type: "text",
      name: "title",
      message: "What is the title of talk?"
    },
    {
      type: "confirm",
      name: "custom",
      message: "Do you want to customize the presentation?",
      initial: false
    }
  ]);
  if (!existsSync(`./${dirName}`))
    mkdirSync(`./${dirName}`);
  const file = Bun.file(`./${dirName}/main.sdf`);
  const writer = file.writer();
  if (response.custom) {
    writer.write(`/::\ncustom_css: custom.css\ncustom_js: custom.js\n::/\n\n`);
  }
  writer.write(`# ${response.title} \n\n## My first Slide`);
  writer.end();
  if (response.custom) {
    Bun.write(`./${dirName}/custom.css`, `
:root {
--sd-heading1-size: 3.75em;
--sd-heading1-line-height: 1;
--sd-heading2-size: 2.25em;
--sd-heading2-line-height: 1;
--sd-heading3-size: 1.75em;
--sd-heading3-line-height: 1;
--sd-text-size: 40px;
--sd-text-line-height: 1.2;

--sd-background-color: #242424;
--sd-heading-color: rgba(255, 255, 255, 0.97);
--sd-text-color: rgba(255, 255, 255, 0.87);
--sd-primary-color: rgb(37, 186, 146);


/* SpeakerView */
--sd-sv-timer-size: 80px;
--sd-sv-text-size: 40px;
--sd-sv-text-line-height: 1.2;
--sd-sv-background-color: #242424;
--sd-sv-text-color: rgba(255, 255, 255, 0.87);
}`);
    Bun.write(`./${dirName}/custom.js`, "");
  }
  process.exit();
};
var create_default = create;

// src/index.js
program.name("slidesk").description("Your presentation companion").version(package_default.version, "-v, --version");
program.command("create").argument("<talk>").action(create_default);
program.argument("<talk>").option("-p, --port <int>", "port", 1337).option("-s, --save", "save the html file").option("-n, --notes", "open with speakers notes").option("-src, --source", "add a button on slides to display its SDF code").option("-g, -gamepad", "control your slide with a gamepad from the presentation").option("--gamepad-sv", "control your slide with a gamepad from the speaker-view").option("-q, --qrcode", "add a QRCode on each slide").option("-t, --timers", "add checkpoint and slide's maximum time on notes view").action(present_default);
program.parse();
